package basic;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		// hasNext : 입력한 값이 있는가 확인
		while (sc.hasNext()) {
			int a, b;
			a = sc.nextInt();
			b = sc.nextInt();

			System.out.println(a + b);

		}
			sc.close();
	}
}
